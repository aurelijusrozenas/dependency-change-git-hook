{
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.3.0",
        "symfony/console": "^5.2",
        "symfony/filesystem": "^5.2",
        "symfony/finder": "^5.2",
        "symfony/process": "^5.2"
    },
    "require-dev": {
        "kint-php/kint": "^3.3",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "5.2.*"
        }
    }
}
